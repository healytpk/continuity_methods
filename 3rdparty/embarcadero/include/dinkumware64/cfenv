using _CSTD fesetround; using _CSTD fegetenv;
using _CSTD feholdexcept; using _CSTD fesetenv;
using _CSTD feupdateenv;

	namespace tr1 {
using _CSTD fenv_t; using _CSTD fexcept_t;
